<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="root" prefHeight="530" prefWidth="800" xmlns:fx="http://javafx.com/fxml/1" stylesheets="/fxml/CSS/style.css" xmlns="http://javafx.com/javafx/8.0.112" fx:controller="logic.RankingController">
    <children>
        <Label fx:id="result" layoutX="270" layoutY="30" minHeight="20" minWidth="69" text="Thank you for playing:">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="top1" layoutX="300" layoutY="200" text="#1:" visible="false">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="top2" layoutX="300" layoutY="240" text="#2:" visible="false">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="top3" layoutX="300" layoutY="280" text="#3:" visible="false">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="top4" layoutX="300" layoutY="320" text="#4:" visible="false">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="top5" layoutX="300" layoutY="360" text="#5:" visible="false">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>

        <Button fx:id="transbutton" layoutX="600" layoutY="420" onAction="#handleButtonAction" text="Page transition" />
        <Button fx:id="rankbutton" layoutX="300.0" layoutY="100.0" onAction="#rankAction" text="Top 5!" />
    </children>
</AnchorPane>