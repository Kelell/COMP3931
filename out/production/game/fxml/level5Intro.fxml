<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="root" prefHeight="530" prefWidth="800" xmlns:fx="http://javafx.com/fxml/1" stylesheets="/fxml/CSS/style.css" xmlns="http://javafx.com/javafx/8.0.112" fx:controller="logic.L5IntroController">
    <children>
        <Label fx:id="result" layoutX="300" layoutY="30" minHeight="20" minWidth="69" text="Scenario Five">
            <font>
                <Font name="Arial" size="30" />
            </font>
        </Label>
        <Label fx:id="line1" layoutX="40" layoutY="70" text="In this scenario your friends are coming to visit for the first time.">
            <font>
                <Font name="Arial" size="20" />
            </font>
        </Label>
        <Label fx:id="line2" layoutX="40" layoutY="110" text="You know what that means- time to check the bathroom!">
            <font>
                <Font name="Arial" size="20" />
            </font>
        </Label>

        <Button fx:id="transbutton" layoutX="600" layoutY="420" onAction="#handleButtonAction" text="Page transition" />
    </children>
</AnchorPane>